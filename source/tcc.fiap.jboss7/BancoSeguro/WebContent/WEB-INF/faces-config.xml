<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    
	<navigation-rule>
		<display-name>gerentes</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>gerentes</from-outcome>
			<to-view-id>/admin/gerentes.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
    
	<navigation-rule>
		<display-name>contas</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>contas</from-outcome>
			<to-view-id>/admin/contas.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>banco</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>banco</from-outcome>
			<to-view-id>/user/banco.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>trustedApps</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>trustedApps</from-outcome>
			<to-view-id>/admin/apps.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

</faces-config>
